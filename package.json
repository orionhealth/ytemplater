    "growl": "~> 1.10.0",
    "gulp-mocha": "~0.4.1",
    "rimraf": "~2.2.6",
    "chai-fs": "0.0.3",
    "cli-spy": "^0.2.1",
    "mock-fs": "^2.2.0",
