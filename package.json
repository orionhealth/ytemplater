  },
  "homepage": "https://github.com/orionhealth/ytemplater",
  "dependencies": {
    "yui": "^3.15.0",
    "q": "~1.0.0",
    "through": "~2.3.4",
    "concat-stream": "~1.4.1",
    "map-stream": "~0.1.0",
    "stream-combiner": "0.0.4",
    "mkdirp": "~0.3.5",
    "yargs": "^1.2.1",
    "lodash": ">=4.17.5"
  },
  "devDependencies": {
    "chai": "~1.9.0",
    "istanbul": "~0.2.4",
    "mocha": "^1.18.0",
