{
  "name": "ytemplater",
  "version": "0.1.1",
  "description": "Node.js utility for precompiling templates for use with YUI's Y.Template.",
  "main": "./lib/ytemplater.js",
  "bin": {
    "ytemplater": "./bin/ytemplater"
  },
  "keywords": [
    "yui",
    "build",
    "cli",
    "tool",
    "template",
    "handlebars",
    "precompile"
